{
  "name": "css-to-rfs",
  "displayName": "css-to-rfs",
  "description": "Convert CSS declarations to their RFS representation and back",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.109.0"
  },
  "repository": {
    "url": "https://github.com/janniksohn/css-to-rfs"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "css-to-rfs.convert",
        "title": "Convert CSS to RFS"
      }
    ],
    "keybindings": [
      {
        "command": "css-to-rfs.convert",
        "key": "ctrl+shift+r",
        "mac": "cmd+shift+r",
        "when": "editorTextFocus && editorHasSelection"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.109.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.54.0",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
